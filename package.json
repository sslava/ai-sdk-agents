{
  "name": "ai-sdk-agents",
  "version": "0.4.15",
  "description": "Vercel AI SDK Simple Agents",
  "license": "MIT",
  "author": "Iaroslav Sobolev <sobolevslava@gmail.com>",
  "private": false,
  "keywords": [
    "ai",
    "sdk",
    "agents",
    "vercel"
  ],
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE.md",
    "package.json",
    "tsconfig.json"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.mjs"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sslava/ai-sdk-agents.git"
  },
  "bugs": {
    "url": "https://github.com/sslava/ai-sdk-agents/issues"
  },
  "homepage": "https://github.com/sslava/ai-sdk-agents#readme",
  "scripts": {
    "build": "tsc && tsup",
    "test": "vitest",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "version": "changeset version",
    "release": "changeset publish",
    "prepare": "husky"
  },
  "peerDependencies": {
    "ai": "^4.3.10"
  },
  "devDependencies": {
    "@ai-sdk/openai": "^2.0.1",
    "@changesets/cli": "^2.29.5",
    "@eslint/js": "^9.30.0",
    "@types/node": "^24.0.7",
    "@vitest/coverage-v8": "^3.2.4",
    "ai": "^4.3.16",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vitest": "^3.2.4",
    "zod": "^4.0.14"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "pnpm lint:fix"
    ]
  },
  "packageManager": "pnpm@10.11.0"
}
