{
  "name": "ai-sdk-agents",
  "version": "0.4.8",
  "description": "Vercel AI SDK Simple Agents",
  "license": "MIT",
  "author": "Iaroslav Sobolev <sobolevslava@gmail.com>",
  "private": false,
  "keywords": [
    "ai",
    "sdk",
    "agents",
    "vercel"
  ],
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE.md",
    "package.json",
    "tsconfig.json"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.mjs"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "scripts": {
    "build": "tsc && tsup",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "version": "changeset version",
    "release": "changeset publish",
    "prepare": "husky"
  },
  "peerDependencies": {
    "ai": "^4.3.10"
  },
  "devDependencies": {
    "@ai-sdk/openai": "^1.3.20",
    "@changesets/cli": "^2.29.3",
    "@eslint/js": "^9.25.1",
    "@types/node": "^22.15.16",
    "ai": "^4.3.15",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "tsc": "^2.0.4",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vitest": "^3.1.2",
    "zod": "^3.24.4"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "yarn lint:fix"
    ]
  },
  "packageManager": "yarn@1.22.22"
}
